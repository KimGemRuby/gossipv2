<h1>New Utilisateur</h1>

<%= render 'form', utilisateur: @utilisateur %>

<%= link_to 'Back', utilisateurs_path %>
<% user_id = rand(User.first.id..User.last.id) %>

<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.submit %>
<%end%>